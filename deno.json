{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "strict": true,
    "allowJs": false,
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": {
    "options": {
      "useTabs": false,
      "lineWidth": 120,
      "indentWidth": 2,
      "singleQuote": false,
      "semiColons": false
    }
  }
}
